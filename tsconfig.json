{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES6",
    "rootDir": "src-ts",
    "outDir": "src-js",
    "jsx": "react",
    "jsxFactory": "Mini.element",
    "jsxFragmentFactory": "Mini.fragment",
    "moduleResolution": "node",
    "baseUrl": ".",
    // "paths": {
    //   "@/*": ["./src-js/mini/*"]
    // },
    "skipLibCheck": false,
  },
  "include": [
    "src-ts/**/*"
  ],
  "exclude": [
    "node_modules",
    "src-js"
  ]
}
